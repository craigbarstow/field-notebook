<div class="small-12 columns img-wrapper" id="img-upload-wrapper">
  <h1>Add a Photo</h1>
  <%= form_for [:project, @photo], multiple: true do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.file_field :image %>

    <%= f.label :caption %>
    <%= f.text_area :caption %>

    <%= f.submit "Add Photo", class: "button radius"  %>
  <% end %>
